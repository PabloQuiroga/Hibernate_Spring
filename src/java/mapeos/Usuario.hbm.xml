<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojos.Usuario" table="usuarios" >
        <id column="email" name="mail" access="field" />
        <property column="nickname" name="nombre" access="field" />
        <property column="pass" name="pass" access="field" />
    </class>
</hibernate-mapping>