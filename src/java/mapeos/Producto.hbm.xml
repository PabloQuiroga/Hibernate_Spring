<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojos.Producto" table="productos" >
        <id column="codigo" name="codigo" access="field" />
        <property column="nombre" name="nombre" access="field" />
        <property column="tipo" name="tipo" access="field" />
        <property column="unidad_venta" name="unidad_venta" access="field" />
        <property column="precio" name="precio" access="field" />
        <property column="cantidad" name="cantidad" access="field" />
    </class>
</hibernate-mapping>
